





	THEORY EQUT


newparent `PROPT` ;;

newconstant ( `EQ` , ":*->(*->tr)" ) ;;

NEWAXIOMS();;

AXSTR1  "!x:*. EQ x (UU:*) == UU:tr"

AXSTR2  "!y:*. !x:*. EQ x y == UU:tr & (x == UU:* IMP TT == FF) IMP y == UU:*"

AXREFL  "!x:*. (x == UU:* IMP TT == FF) IMP EQ x x == TT"

AXEQ1  "!y:*. !x:*. EQ x y == TT IMP x == y"

AXEQ2  "!y:*. !x:*. EQ x y == FF & x == y IMP TT == FF"

AXEQ3  "!y:*. !x:*. EQ x x == TT & EQ y y == TT & x == y IMP EQ x y == TT"

AXEQ4  "!y:*. !x:*. EQ x x == TT & EQ y y == TT & (x == y IMP TT == FF) IMP EQ x y == FF"

AXCOMT  "!y:*. !x:*. EQ x y == EQ y x :tr"

AXFLAT  "!y:*. !x:*. EQ x y == FF & x << y IMP TT == FF"
